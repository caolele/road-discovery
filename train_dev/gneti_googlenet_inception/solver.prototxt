net: "train.prototxt"

test_iter: 32
test_interval: 500

solver_type: SGD
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 5000
momentum: 0.9
weight_decay: 0.0005

display: 10
max_iter: 500000

snapshot: 500
snapshot_prefix: "snapshots/goneti"

solver_mode: GPU # change to CPU is you do not have GPU
device_id: 0

#random_seed: 1701
#debug_info: false
