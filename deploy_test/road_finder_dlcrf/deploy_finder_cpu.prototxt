### SUM the four branches
layer {
  bottom: "fc8_exper_1"
  bottom: "fc8_exper_2"
  bottom: "fc8_exper_3"
  bottom: "fc8_exper_4"
  top: "fc8_exper"
  name: "fc8_exper"
  type: "Eltwise"
  eltwise_param {
    operation: SUM
  }
}
layer {
  name: "upsample"
  type: "Deconvolution"
  bottom: "fc8_exper"
  top: "upscore"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution_param {
    kernel_size: 17
    stride: 8
    num_output: 2
    group: 2
    pad: 4
    weight_filler {
      type: "bilinear"
    }
    bias_term: false
  }
}

layer {
  name: "score"
  type: "Crop"
  bottom: "upscore"
  bottom: "data"
  top: "score"
}

###CRF###
layer {
  type: 'Split'
  name: 'splitting'
  bottom: 'score'
  top: 'unary'
  top: 'Q0'
}

layer {
  name: "inference"
  type: "MultiStageMeanfield"
  bottom: "unary"
  bottom: "Q0"
  bottom: "data"
  top: "pred"
  param {
    lr_mult: 10000
  }
  param {
    lr_mult: 10000
  }
  param {
    lr_mult: 1000
  }
  multi_stage_meanfield_param {
   num_iterations: 10
   compatibility_mode: POTTS
   threshold: 2
   theta_alpha: 1200
   theta_beta: 5
   theta_gamma: 24
   #spatial_filter_weights_str: "3.0 3.0"
   #bilateral_filter_weights_str: "5.0 5.0"
   spatial_filter_weight: 3
   bilateral_filter_weight: 5
  }
}

#################
layer {
  name: "prob1"
  type: "Softmax"
  bottom: "score"
  top: "prob1"
}

layer {
  name: "prob2"
  type: "Softmax"
  bottom: "pred"
  top: "prob2"
}
